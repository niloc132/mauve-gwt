classes.stamp: choices
	here=`/bin/pwd`; cd $(srcdir); \
	  CLASSPATH=$$here:`/bin/pwd` $(JAVAC) $(JCFLAGS) -d $$here \
	    $(harness_files) `cat $$here/choices`
	touch classes.stamp
choices: FORCE
	ok=no; \
	if test -f .save-keys && test "`cat .save-keys`" = "$(KEYS)"; then \
	  ok=yes; \
	fi; \
	here=`/bin/pwd`; \
	if test "$$ok" = no; then \
	  echo "$(KEYS)" > .save-keys; \
	  cd $(srcdir) && $(SHELL) choose $$here $(KEYS); \
	fi

FORCE:

@USE_GCJ_TRUE@.class.o:
@USE_GCJ_TRUE@	CLASSPATH=.:$(srcdir) $(GCJ) -fassume-compiled $(JC1FLAGS) -c -o $@ $<

@USE_GCJ_TRUE@check-local: $(check_PROGRAMS) choices
@USE_GCJ_TRUE@	cat classes | ./SimpleTestHarness

@USE_GCJ_TRUE@recheck:
@USE_GCJ_TRUE@	test -f .save-keys || : > .save-keys
@USE_GCJ_TRUE@	@rm -f SimpleTestHarness
@USE_GCJ_TRUE@	$(MAKE) KEYS="`cat .save-keys`" check

@USE_GCJ_FALSE@check-local: classes.stamp
@USE_GCJ_FALSE@	cat classes | $(JAVA) gnu.testlet.SimpleTestHarness

@USE_GCJ_FALSE@recheck:
@USE_GCJ_FALSE@	test -f .save-keys || : > .save-keys
@USE_GCJ_FALSE@	$(MAKE) KEYS="`cat .save-keys`" check

